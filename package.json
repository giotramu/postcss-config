{
  "name": "@giotramu/postcss-config",
  "version": "4.3.0",
  "description": "Flexible PostCSS config that combines useful plugins.",
  "author": "giotramu <https://github.com/giotramu>",
  "homepage": "https://github.com/giotramu/postcss-config#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/giotramu/postcss-config.git"
  },
  "bugs": {
    "url": "https://github.com/giotramu/postcss-config/issues"
  },
  "engines": {
    "node": "16.x || 18.x",
    "npm": "8.x || 9.x"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "types": "types/index.d.ts",
  "files": [
    "/index.js",
    "/extends.js",
    "/core/",
    "/types/"
  ],
  "prettier": "@giotramu/prettier-config",
  "scripts": {
    "check:tsc": "tsc --noEmit",
    "check:src": "eslint . --ext .js,.ts",
    "check": "npm run check:tsc && npm run check:src",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "del ./{index,extends}.js ./common ./types",
    "build": "npm run clean && tsc",
    "precompile:css": "npm run build",
    "compile:css": "postcss ./examples/styles.pcss --dir=examples/ --ext=min.css"
  },
  "devDependencies": {
    "@evilmartians/lefthook": "^1.3.0",
    "@giotramu/eslint-config": "^1.1.0",
    "@giotramu/prettier-config": "^1.5.0",
    "@giotramu/tsc": "^0.3.0",
    "@types/node": "^16.0.0",
    "@vitest/coverage-c8": "^0.30.0",
    "del-cli": "^5.0.0",
    "eslint": "^8.38.0",
    "postcss-cli": "^10.1.0",
    "prettier": "^2.8.0",
    "typescript": "^5.0.0",
    "utility-types": "^3.10.0",
    "vitest": "^0.30.0"
  },
  "dependencies": {
    "autoprefixer": "10.4.14",
    "cssnano": "6.0.0",
    "deepmerge": "4.3.1",
    "postcss": "8.4.21",
    "postcss-custom-media": "9.1.2",
    "postcss-inline-svg": "6.0.0",
    "postcss-reporter": "7.0.5"
  },
  "keywords": [
    "css",
    "postcss",
    "postcss-config"
  ]
}
