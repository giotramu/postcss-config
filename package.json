{
  "name": "@giotramu/postcss-config",
  "version": "2.3.1",
  "description": "Flexible PostCSS config with great defaults.",
  "author": "giotramu <https://twitter.com/giotramu>",
  "homepage": "https://github.com/giotramu/postcss-config#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/giotramu/postcss-config.git"
  },
  "bugs": {
    "url": "https://github.com/giotramu/postcss-config/issues"
  },
  "engines": {
    "node": ">=10.x",
    "npm": ">=6.x"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "check:src": "tsc --noEmit",
    "lint:src": "eslint . --ext .ts,.js",
    "lint": "npm run lint:src",
    "test": "jest --coverage",
    "prebuild": "rimraf ./dist",
    "build": "tsc",
    "precompile:css": "npm run build",
    "compile:css": "postcss ./examples/styles.scss --dir=examples/ --ext=min.css --verbose"
  },
  "dependencies": {
    "@csstools/postcss-sass": "^4.0.0",
    "cssnano": "^4.1.10",
    "cssnano-preset-advanced": "^4.0.7",
    "deepmerge": "^4.2.2",
    "postcss": "^7.0.27",
    "postcss-custom-media": "^7.0.8",
    "postcss-reporter": "^6.0.1",
    "postcss-scss": "^2.0.0"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.8",
    "babel-jest": "^25.2.6",
    "eslint": "^6.8.0",
    "eslint-config-contactlab": "^4.2.2",
    "eslint-config-prettier": "^6.10.1",
    "husky": "^4.2.3",
    "jest": "^25.2.6",
    "postcss-cli": "^7.1.0",
    "prettier": "^2.0.2",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.3.0",
    "typescript": "^3.8.3"
  },
  "keywords": [
    "css",
    "postcss",
    "postcss-config",
    "sass",
    "scss"
  ]
}
