{
  "name": "@giotramu/postcss-config",
  "version": "4.0.1",
  "description": "Flexible PostCSS config with great defaults. It combines Dart SCSS with your favorite PostCSS plugins.",
  "author": "giotramu <https://twitter.com/giotramu>",
  "homepage": "https://github.com/giotramu/postcss-config#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/giotramu/postcss-config.git"
  },
  "bugs": {
    "url": "https://github.com/giotramu/postcss-config/issues"
  },
  "engines": {
    "node": ">=12.x",
    "npm": ">=6.x"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "types": "types/index.d.ts",
  "files": [
    "{index,extends}.js",
    "/common/",
    "/types/",
    "CHANGELOG.md"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint:src": "eslint . --ext .js,.ts",
    "lint": "npm run lint:src",
    "test": "jest",
    "clean": "del ./{index,extends}.js ./common ./types",
    "build": "npm run clean && tsc",
    "precompile:css": "npm run build",
    "compile:css": "postcss ./examples/styles.pcss --dir=examples/ --ext=min.css"
  },
  "prettier": "@giotramu/prettier-config",
  "dependencies": {
    "autoprefixer": "10.3.6",
    "cssnano": "5.0.8",
    "deepmerge": "4.2.2",
    "postcss": "8.3.8",
    "postcss-custom-media": "8.0.0",
    "postcss-inline-svg": "5.0.0",
    "postcss-reporter": "7.0.4"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.7.7",
    "@giotramu/prettier-config": "^1.2.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.2",
    "del-cli": "^4.0.1",
    "eslint": "^7.32.0",
    "eslint-config-contactlab": "^7.0.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^27.2.4",
    "jest-junit": "^13.0.0",
    "postcss-cli": "^9.0.1",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3",
    "utility-types": "^3.10.0"
  },
  "keywords": [
    "css",
    "postcss",
    "postcss-config",
    "sass",
    "scss"
  ]
}
