{
  "name": "@giotramu/postcss-config",
  "version": "4.2.0",
  "description": "Flexible PostCSS config that combines useful plugins.",
  "author": "giotramu <https://github.com/giotramu>",
  "homepage": "https://github.com/giotramu/postcss-config#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/giotramu/postcss-config.git"
  },
  "bugs": {
    "url": "https://github.com/giotramu/postcss-config/issues"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "types": "types/index.d.ts",
  "files": [
    "{index,extends}.js",
    "/core/",
    "/types/"
  ],
  "prettier": "@giotramu/prettier-config",
  "scripts": {
    "check:tsc": "tsc --noEmit",
    "check:src": "eslint . --ext .js,.ts",
    "check": "npm run check:tsc && npm run check:src",
    "test": "jest",
    "clean": "del ./{index,extends}.js ./common ./types",
    "build": "npm run clean && tsc",
    "precompile:css": "npm run build",
    "compile:css": "postcss ./examples/styles.pcss --dir=examples/ --ext=min.css"
  },
  "devDependencies": {
    "@evilmartians/lefthook": "^1.2.0",
    "@giotramu/eslint-config": "^1.0.0",
    "@giotramu/prettier-config": "^1.4.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^16.0.0",
    "del-cli": "^5.0.0",
    "eslint": "^8.36.0",
    "jest": "^29.4.0",
    "jest-junit": "^15.0.0",
    "postcss-cli": "^10.1.0",
    "prettier": "^2.8.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^4.9.0",
    "utility-types": "^3.10.0"
  },
  "dependencies": {
    "autoprefixer": "10.4.13",
    "cssnano": "5.1.15",
    "deepmerge": "4.3.0",
    "postcss": "8.4.21",
    "postcss-custom-media": "9.1.2",
    "postcss-inline-svg": "6.0.0",
    "postcss-reporter": "7.0.5"
  },
  "keywords": [
    "css",
    "postcss",
    "postcss-config"
  ]
}
