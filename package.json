{
  "name": "@giotramu/postcss-config",
  "version": "3.4.0",
  "description": "Flexible PostCSS config with great defaults. It combines Dart SCSS with your favorite PostCSS plugins.",
  "author": "giotramu <https://twitter.com/giotramu>",
  "homepage": "https://github.com/giotramu/postcss-config#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/giotramu/postcss-config.git"
  },
  "bugs": {
    "url": "https://github.com/giotramu/postcss-config/issues"
  },
  "engines": {
    "node": ">=12.x",
    "npm": ">=6.x"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "types": "typings/index.d.ts",
  "files": [
    "{index,extends}.js",
    "common/",
    "typings/"
  ],
  "scripts": {
    "check": "tsc --noEmit",
    "lint:src": "eslint . --ext .js,.ts",
    "lint": "npm run lint:src",
    "pretest": "npm run check & npm run lint",
    "test": "jest --coverage",
    "clean": "rimraf ./{index,extends}.js ./common ./typings",
    "prebuild": "npm run clean",
    "build": "tsc",
    "precompile:css": "npm run build",
    "compile:css": "postcss ./examples/styles.scss --dir=examples/ --ext=min.css"
  },
  "peerDependencies": {
    "postcss": "^8.0.9"
  },
  "dependencies": {
    "@csstools/postcss-sass": "^4.0.0",
    "autoprefixer": "^10.0.0",
    "cssnano": "^4.1.10",
    "deepmerge": "^4.2.2",
    "postcss-custom-media": "^7.0.8",
    "postcss-inline-svg": "^4.1.0",
    "postcss-reporter": "^7.0.0",
    "postcss-scss": "^3.0.1",
    "postcss-selector-not": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "babel-jest": "^26.3.0",
    "eslint": "^7.9.0",
    "eslint-config-contactlab": "^5.0.0",
    "eslint-config-prettier": "^6.11.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "postcss": "^8.0.9",
    "postcss-cli": "^8.0.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.0.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.0",
    "typescript": "^4.0.3",
    "utility-types": "^3.10.0"
  },
  "keywords": [
    "css",
    "postcss",
    "postcss-config",
    "sass",
    "scss"
  ]
}
