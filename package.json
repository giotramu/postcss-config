{
  "name": "@giotramu/postcss-config",
  "version": "4.1.1",
  "description": "Flexible PostCSS config with great defaults. It combines Dart SCSS with your favorite PostCSS plugins.",
  "author": "giotramu <https://github.com/giotramu>",
  "homepage": "https://github.com/giotramu/postcss-config#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/giotramu/postcss-config.git"
  },
  "bugs": {
    "url": "https://github.com/giotramu/postcss-config/issues"
  },
  "engines": {
    "node": ">=14",
    "npm": ">=8"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "types": "types/index.d.ts",
  "files": [
    "{index,extends}.js",
    "/common/",
    "/types/",
    "CHANGELOG.md"
  ],
  "scripts": {
    "check:tsc": "tsc --noEmit",
    "check:src": "eslint . --ext .js,.ts",
    "check": "npm run check:tsc && npm run check:src",
    "test": "jest",
    "clean": "del ./{index,extends}.js ./common ./types",
    "build": "npm run clean && tsc",
    "precompile:css": "npm run build",
    "compile:css": "postcss ./examples/styles.pcss --dir=examples/ --ext=min.css"
  },
  "prettier": "@giotramu/prettier-config",
  "dependencies": {
    "autoprefixer": "10.4.8",
    "cssnano": "5.1.13",
    "deepmerge": "4.2.2",
    "postcss": "8.4.16",
    "postcss-custom-media": "8.0.2",
    "postcss-inline-svg": "5.0.0",
    "postcss-reporter": "7.0.5"
  },
  "devDependencies": {
    "@evilmartians/lefthook": "^1.1.1",
    "@giotramu/prettier-config": "^1.4.0",
    "@types/jest": "^28.1.7",
    "@types/node": "^16.11.54",
    "del-cli": "^5.0.0",
    "eslint": "^8.23.0",
    "eslint-config-contactlab": "^9.0.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^28.1.3",
    "jest-junit": "^14.0.1",
    "postcss-cli": "^10.0.0",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2",
    "utility-types": "^3.10.0"
  },
  "keywords": [
    "css",
    "postcss",
    "postcss-config"
  ]
}
