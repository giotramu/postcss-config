{
  "name": "@giotramu/postcss-config",
  "version": "3.4.4",
  "description": "Flexible PostCSS config with great defaults. It combines Dart SCSS with your favorite PostCSS plugins.",
  "author": "giotramu <https://twitter.com/giotramu>",
  "homepage": "https://github.com/giotramu/postcss-config#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/giotramu/postcss-config.git"
  },
  "bugs": {
    "url": "https://github.com/giotramu/postcss-config/issues"
  },
  "engines": {
    "node": ">=12.x",
    "npm": ">=6.x"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "types": "typings/index.d.ts",
  "files": [
    "{index,extends}.js",
    "common/",
    "typings/",
    "CHANGELOG.md"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint:src": "eslint . --ext .js,.ts",
    "lint": "npm run lint:src",
    "test": "jest",
    "clean": "rimraf ./{index,extends}.js ./common ./typings",
    "prebuild": "npm run clean",
    "build": "tsc",
    "precompile:css": "npm run build",
    "compile:css": "postcss ./examples/styles.scss --dir=examples/ --ext=min.css"
  },
  "prettier": "@giotramu/prettier-config",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": "prettier --write",
    "*.{ts,tsx}": "eslint --fix"
  },
  "peerDependencies": {
    "postcss": "^7.0.32"
  },
  "dependencies": {
    "@csstools/postcss-sass": "^4.0.0",
    "autoprefixer": "^9.8.6",
    "cssnano": "^4.1.10",
    "deepmerge": "^4.2.2",
    "postcss-custom-media": "^7.0.8",
    "postcss-inline-svg": "^4.1.0",
    "postcss-reporter": "^6.0.1",
    "postcss-scss": "^2.1.1",
    "postcss-selector-not": "^4.0.0"
  },
  "devDependencies": {
    "@giotramu/prettier-config": "^1.1.1",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.12",
    "eslint": "^7.15.0",
    "eslint-config-contactlab": "^6.0.0",
    "eslint-config-prettier": "^7.0.0",
    "husky": "^5.1.2",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "lint-staged": "^10.5.3",
    "postcss-cli": "^8.3.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "utility-types": "^3.10.0"
  },
  "keywords": [
    "css",
    "postcss",
    "postcss-config",
    "sass",
    "scss"
  ]
}
